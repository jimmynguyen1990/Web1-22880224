{{#each data}}
  <h1 class="py-3">{{name}}</h1>
  <div>
    <img
      src="{{thumbpath}}"
      alt="{{name}}"
      class="float-start rounded-4 me-3"
    />
    <div class="text-black-50">{{{summary}}}</div>
    <a
      class="btn btn-link readmore p-0"
      href="#aboutus/{{id}}"
      onclick="fetchDetails('aboutus/{{id}}', 'details-template', 'content')"
    >Read more</a>
  </div>
{{/each}}